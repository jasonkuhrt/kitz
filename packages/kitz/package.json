{
  "name": "kitz",
  "version": "0.0.0-kitz-release",
  "description": "A TypeScript standard library",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./build/index.js",
    "./arr": "./build/arr.js",
    "./bool": "./build/bool.js",
    "./date": "./build/date.js",
    "./err": "./build/err.js",
    "./fn": "./build/fn.js",
    "./lang": "./build/lang.js",
    "./null": "./build/null.js",
    "./num": "./build/num.js",
    "./obj": "./build/obj.js",
    "./optic": "./build/optic.js",
    "./pat": "./build/pat.js",
    "./prom": "./build/prom.js",
    "./prox": "./build/prox.js",
    "./rec": "./build/rec.js",
    "./str": "./build/str.js",
    "./ts": "./build/ts.js",
    "./tup": "./build/tup.js",
    "./undefined": "./build/undefined.js",
    "./assert": "./build/assert.js",
    "./bldr": "./build/bldr.js",
    "./cli": "./build/cli.js",
    "./color": "./build/color.js",
    "./conf": "./build/conf.js",
    "./env": "./build/env.js",
    "./fs": "./build/fs.js",
    "./group": "./build/group.js",
    "./html": "./build/html.js",
    "./http": "./build/http.js",
    "./idx": "./build/idx.js",
    "./json": "./build/json.js",
    "./jsonc": "./build/jsonc.js",
    "./log": "./build/log.js",
    "./mod": "./build/mod.js",
    "./name": "./build/name.js",
    "./oak": "./build/oak.js",
    "./pkg": "./build/pkg.js",
    "./paka": "./build/paka.js",
    "./resource": "./build/resource.js",
    "./sch": "./build/sch.js",
    "./semver": "./build/semver.js",
    "./syn": "./build/syn.js",
    "./test": "./build/test.js",
    "./tex": "./build/tex.js",
    "./tree": "./build/tree.js",
    "./url": "./build/url.js",
    "./ware": "./build/ware.js",
    "./monorepo": "./build/monorepo.js"
  },
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "build": "tsgo -p tsconfig.build.json",
    "dev": "tsgo -p tsconfig.build.json --watch",
    "check:types": "tsgo --noEmit",
    "check:lint": "oxlint src/",
    "check:package": "publint && attw --pack --ignore-rules no-resolution cjs-resolves-to-esm internal-resolution-error"
  },
  "dependencies": {
    "@kitz/assert": "workspace:*",
    "@kitz/bldr": "workspace:*",
    "@kitz/cli": "workspace:*",
    "@kitz/color": "workspace:*",
    "@kitz/conf": "workspace:*",
    "@kitz/core": "workspace:*",
    "@kitz/env": "workspace:*",
    "@kitz/fs": "workspace:*",
    "@kitz/group": "workspace:*",
    "@kitz/html": "workspace:*",
    "@kitz/http": "workspace:*",
    "@kitz/idx": "workspace:*",
    "@kitz/json": "workspace:*",
    "@kitz/jsonc": "workspace:*",
    "@kitz/log": "workspace:*",
    "@kitz/mod": "workspace:*",
    "@kitz/monorepo": "workspace:*",
    "@kitz/name": "workspace:*",
    "@kitz/num": "workspace:*",
    "@kitz/oak": "workspace:*",
    "@kitz/pkg": "workspace:*",
    "@kitz/paka": "workspace:*",
    "@kitz/resource": "workspace:*",
    "@kitz/sch": "workspace:*",
    "@kitz/semver": "workspace:*",
    "@kitz/syn": "workspace:*",
    "@kitz/test": "workspace:*",
    "@kitz/tex": "workspace:*",
    "@kitz/tree": "workspace:*",
    "@kitz/url": "workspace:*",
    "@kitz/ware": "workspace:*"
  }
}
